<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kxcs.dao.StaffdepMapper">


   <!-- 通过学院ID查找学院 -->
   <select id="findStaffdepById" resultType="Staffdep">
     select * from staffdep where id=#{id}
   </select>
    <!-- 查找所有学院 -->
    <select id="findAllDepartment" resultType="Staffdep">
       select * from  staffdep
   </select>

</mapper>